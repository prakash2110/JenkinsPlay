@Library('jenkins-library@master') l1
@Library('pipeline-library-demo@master') l2
import com.jenkins.*
pipeline {
    agent any

    stages {
        stage("Env Variables") {
            steps {
                sh "printenv"
		sayHello 'Shree'
		hello1 'Prakash'
		Test test = new Test()
		print test.getName()
            }
        }
    }
}
